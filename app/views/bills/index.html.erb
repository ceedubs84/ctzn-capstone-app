<div><em>Welcome, </em><bold><%= current_user.first_name %></bold></div>

<h2>Here are some Billz!</h2>

<% @bills.each do |bill| %>
  <a href="/bills/<%= (bill["bill_uri"]).gsub('/', '-') %>"><%= bill["number"] %></a>
  <div>Title: <%= bill["title"] %></div>
  <div>
    <button>Add +</button>
  </div>
<% end %>

<div><a href="/logout">Log out!</a></div>